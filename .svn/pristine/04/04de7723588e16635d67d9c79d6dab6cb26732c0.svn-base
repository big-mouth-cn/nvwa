package org.bigmouth.nvwa.utils.exception;

public class SystemRuntimeException extends RuntimeException {

	private static final long serialVersionUID = -6783030002687340756L;

	public SystemRuntimeException() {
		super();
	}

	public SystemRuntimeException(String message, Throwable cause) {
		super(message, cause);
	}

	public SystemRuntimeException(String message) {
		super(message);
	}

	public SystemRuntimeException(Throwable cause) {
		super(cause);
	}
}
